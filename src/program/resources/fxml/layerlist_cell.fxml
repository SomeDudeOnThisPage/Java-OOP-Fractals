<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane id="layer-list-cell" fx:id="root" blendMode="SRC_OVER" focusTraversable="true" minWidth="-1.0" pickOnBounds="false" prefHeight="30.0" prefWidth="-1.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2">
  <center>
    <TextField id="layer-list-cell-text-field" fx:id="textField" blendMode="SRC_OVER" depthTest="INHERIT" editable="true" onAction="#onTextFieldAction" onMouseClicked="#onTextFieldClick" prefWidth="-1.0" promptText="Layer Name" style="" text="Layer">
      <BorderPane.margin>
        <Insets left="6.0" right="6.0" />
      </BorderPane.margin>
    </TextField>
  </center>
  <left>
    <CheckBox fx:id="visibleCheckBox" alignment="CENTER_LEFT" disable="false" mnemonicParsing="false" onAction="#onVisibleCheckBox" text="" BorderPane.alignment="CENTER">
      <BorderPane.margin>
        <Insets left="6.0" />
      </BorderPane.margin>
    </CheckBox>
  </left>
</BorderPane>
